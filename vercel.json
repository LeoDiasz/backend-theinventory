{
    "version": 2,
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/src/app.ts" }]
}